<h1 class="text-center">旅に出かけよう！</h1>
	<div class="margin-top30">
		<h3 class="col-xs-offset-2 margin">人気の投稿から見つける</h3>
		<ul class="slider">
			<% @all_ranks.each do |post| %>
				<li class="inview">
					<%= link_to attachment_image_tag(post,:post_image, format: "jpeg", class: "img-zoom bx-wrapper"), post_path(post) %></li>
			<% end %>
		</ul>
	</div>

	<div class="margin-top30">
		<h3 class="col-xs-offset-2">キーワードから検索する</h3>
		<div class="search-container">
			<div class="search-form text-center">
				<div class="">
					<%= form_tag('/', method: :get, class:"search-form", placeholder:'',remote: true) do %>
						<%= text_field_tag :body, :'', class: "search-input fa", placeholder: "\uF002", autocomplete:'off' %>
					<% end %>
					<div class="search-result"></div>
				</div>
			</div>
		</div>
	</div>




